<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pengetahuankategori">

    <ImageButton
        android:id="@+id/pengetahuanQ"
        android:layout_width="412dp"
        android:layout_height="134dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/quiz_it_removebg_preview"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/bendera"
        android:layout_width="151dp"
        android:layout_height="184dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="64dp"
        android:background="#0037FF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pengetahuanQ"
        app:srcCompat="@drawable/bendera_removebg_preview__1_" />

</androidx.constraintlayout.widget.ConstraintLayout>